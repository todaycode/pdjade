
items = ['1','2','3']

ul
  - for item in items
    li= item

ul
  for i in items
    li(class='item-#{i}')= i

ul
  each item in items
    li= item

ul
  items = ["one", "two", "three"]
  each item, i in items
    li #{item}: #{i}

nums = [1, 2, 3]
letters = ['a', 'b', 'c']

ul
  for l in letters
    for n in nums
      li #{n}: #{l}